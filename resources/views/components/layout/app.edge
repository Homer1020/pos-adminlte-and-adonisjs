<script>
  $('.select2').select2({
    theme: 'bootstrap4'
  })

  $dynamicModal = $('#dynamicModal')

  $('.dynamicModal').on('click', async function(e) {
    const $button = $(e.currentTarget)
    const path = $button.data('path')
    if(!path) return
    const resp = await (await fetch(path)).text()
    if(!resp) return
    $dynamicModal.html(resp)
    $dynamicModal.modal('show')
  })
</script>

